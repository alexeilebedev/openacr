g++ -x c++ -Wno-invalid-offsetof -ffunction-sections -fdata-sections -Wsequence-point -Wno-multichar -Wall -Wshadow -pipe -Wextra -Winit-self -Wmissing-include-dirs -Wmissing-declarations -Winvalid-pch -iquote . -Wno-vla -fpch-preprocess -Wno-class-memaccess -Wpedantic -c cpp/lib/algo/iohook.cpp -o build/CYGWIN_NT-g++.release-x86_64/cpp.lib.algo.iohook.o -msse4.2 -D AOS_SSE42 -O3 -fno-strict-aliasing -Werror -D_MT -D _GNU_SOURCE -Wno-cast-function-type
g++ -x c++ -Wno-invalid-offsetof -ffunction-sections -fdata-sections -Wsequence-point -Wno-multichar -Wall -Wshadow -pipe -Wextra -Winit-self -Wmissing-include-dirs -Wmissing-declarations -Winvalid-pch -iquote . -Wno-vla -fpch-preprocess -Wno-class-memaccess -Wpedantic -c cpp/lib/algo/lib.cpp -o build/CYGWIN_NT-g++.release-x86_64/cpp.lib.algo.lib.o -msse4.2 -D AOS_SSE42 -O3 -fno-strict-aliasing -Werror -D_MT -D _GNU_SOURCE -Wno-cast-function-type
