#!/usr/bin/env bash
echo '# this script has been created using atf_norm bootstrap'
echo '# now building an abt executable which will build the rest'
mkdir -p build
set -e
if [ ! -f .ffroot ]; then echo 'Missing .ffroot. Wrong directory?'; exit 1; fi

echo '# setting up soft link build/coverage (default)'
rm -f build/coverage
mkdir -p build/CYGWIN_NT-g++.coverage-x86_64 # create target directory
ln -s CYGWIN_NT-g++.coverage-x86_64 build/coverage # and a soft link to it

echo '# setting up soft link build/debug (default)'
rm -f build/debug
mkdir -p build/CYGWIN_NT-g++.debug-x86_64 # create target directory
ln -s CYGWIN_NT-g++.debug-x86_64 build/debug # and a soft link to it

echo '# setting up soft link build/profile (default)'
rm -f build/profile
mkdir -p build/CYGWIN_NT-g++.profile-x86_64 # create target directory
ln -s CYGWIN_NT-g++.profile-x86_64 build/profile # and a soft link to it

echo '# setting up soft link build/release (default)'
rm -f build/release
mkdir -p build/CYGWIN_NT-g++.release-x86_64 # create target directory
ln -s CYGWIN_NT-g++.release-x86_64 build/release # and a soft link to it
set -e
set -x
mkdir -p temp
mkdir -p build/release || true
mkdir -p build/CYGWIN_NT-g++.release-x86_64 || true
g++ -x c++ -Wno-invalid-offsetof -ffunction-sections -fdata-sections -Wsequence-point -Wno-multichar -Wall -Wshadow -pipe -Wextra -Winit-self -Wmissing-include-dirs -Wmissing-declarations -Winvalid-pch -iquote . -Wno-vla -fpch-preprocess -Wno-class-memaccess -Wpedantic -c cpp/abt/build.cpp -o build/CYGWIN_NT-g++.release-x86_64/cpp.abt.build.o -msse4.2 -D AOS_SSE42 -O3 -fno-strict-aliasing -Werror -D_MT -D _GNU_SOURCE -Wno-cast-function-type
g++ -x c++ -Wno-invalid-offsetof -ffunction-sections -fdata-sections -Wsequence-point -Wno-multichar -Wall -Wshadow -pipe -Wextra -Winit-self -Wmissing-include-dirs -Wmissing-declarations -Winvalid-pch -iquote . -Wno-vla -fpch-preprocess -Wno-class-memaccess -Wpedantic -c cpp/abt/disas.cpp -o build/CYGWIN_NT-g++.release-x86_64/cpp.abt.disas.o -msse4.2 -D AOS_SSE42 -O3 -fno-strict-aliasing -Werror -D_MT -D _GNU_SOURCE -Wno-cast-function-type
g++ -x c++ -Wno-invalid-offsetof -ffunction-sections -fdata-sections -Wsequence-point -Wno-multichar -Wall -Wshadow -pipe -Wextra -Winit-self -Wmissing-include-dirs -Wmissing-declarations -Winvalid-pch -iquote . -Wno-vla -fpch-preprocess -Wno-class-memaccess -Wpedantic -c cpp/abt/main.cpp -o build/CYGWIN_NT-g++.release-x86_64/cpp.abt.main.o -msse4.2 -D AOS_SSE42 -O3 -fno-strict-aliasing -Werror -D_MT -D _GNU_SOURCE -Wno-cast-function-type
g++ -x c++ -Wno-invalid-offsetof -ffunction-sections -fdata-sections -Wsequence-point -Wno-multichar -Wall -Wshadow -pipe -Wextra -Winit-self -Wmissing-include-dirs -Wmissing-declarations -Winvalid-pch -iquote . -Wno-vla -fpch-preprocess -Wno-class-memaccess -Wpedantic -c cpp/abt/opt.cpp -o build/CYGWIN_NT-g++.release-x86_64/cpp.abt.opt.o -msse4.2 -D AOS_SSE42 -O3 -fno-strict-aliasing -Werror -D_MT -D _GNU_SOURCE -Wno-cast-function-type
g++ -x c++ -Wno-invalid-offsetof -ffunction-sections -fdata-sections -Wsequence-point -Wno-multichar -Wall -Wshadow -pipe -Wextra -Winit-self -Wmissing-include-dirs -Wmissing-declarations -Winvalid-pch -iquote . -Wno-vla -fpch-preprocess -Wno-class-memaccess -Wpedantic -c cpp/gen/abt_gen.cpp -o build/CYGWIN_NT-g++.release-x86_64/cpp.gen.abt_gen.o -msse4.2 -D AOS_SSE42 -O3 -fno-strict-aliasing -Werror -D_MT -D _GNU_SOURCE -Wno-cast-function-type
g++ -x c++ -Wno-invalid-offsetof -ffunction-sections -fdata-sections -Wsequence-point -Wno-multichar -Wall -Wshadow -pipe -Wextra -Winit-self -Wmissing-include-dirs -Wmissing-declarations -Winvalid-pch -iquote . -Wno-vla -fpch-preprocess -Wno-class-memaccess -Wpedantic -c cpp/gen/algo_lib_gen.cpp -o build/CYGWIN_NT-g++.release-x86_64/cpp.gen.algo_lib_gen.o -msse4.2 -D AOS_SSE42 -O3 -fno-strict-aliasing -Werror -D_MT -D _GNU_SOURCE -Wno-cast-function-type
g++ -x c++ -Wno-invalid-offsetof -ffunction-sections -fdata-sections -Wsequence-point -Wno-multichar -Wall -Wshadow -pipe -Wextra -Winit-self -Wmissing-include-dirs -Wmissing-declarations -Winvalid-pch -iquote . -Wno-vla -fpch-preprocess -Wno-class-memaccess -Wpedantic -c cpp/lib/algo/arg.cpp -o build/CYGWIN_NT-g++.release-x86_64/cpp.lib.algo.arg.o -msse4.2 -D AOS_SSE42 -O3 -fno-strict-aliasing -Werror -D_MT -D _GNU_SOURCE -Wno-cast-function-type
g++ -x c++ -Wno-invalid-offsetof -ffunction-sections -fdata-sections -Wsequence-point -Wno-multichar -Wall -Wshadow -pipe -Wextra -Winit-self -Wmissing-include-dirs -Wmissing-declarations -Winvalid-pch -iquote . -Wno-vla -fpch-preprocess -Wno-class-memaccess -Wpedantic -c cpp/lib/algo/backtrace.cpp -o build/CYGWIN_NT-g++.release-x86_64/cpp.lib.algo.backtrace.o -msse4.2 -D AOS_SSE42 -O3 -fno-strict-aliasing -Werror -D_MT -D _GNU_SOURCE -Wno-cast-function-type
g++ -x c++ -Wno-invalid-offsetof -ffunction-sections -fdata-sections -Wsequence-point -Wno-multichar -Wall -Wshadow -pipe -Wextra -Winit-self -Wmissing-include-dirs -Wmissing-declarations -Winvalid-pch -iquote . -Wno-vla -fpch-preprocess -Wno-class-memaccess -Wpedantic -c cpp/lib/algo/cpu_hz.cpp -o build/CYGWIN_NT-g++.release-x86_64/cpp.lib.algo.cpu_hz.o -msse4.2 -D AOS_SSE42 -O3 -fno-strict-aliasing -Werror -D_MT -D _GNU_SOURCE -Wno-cast-function-type
g++ -x c++ -Wno-invalid-offsetof -ffunction-sections -fdata-sections -Wsequence-point -Wno-multichar -Wall -Wshadow -pipe -Wextra -Winit-self -Wmissing-include-dirs -Wmissing-declarations -Winvalid-pch -iquote . -Wno-vla -fpch-preprocess -Wno-class-memaccess -Wpedantic -c cpp/lib/algo/crc32.cpp -o build/CYGWIN_NT-g++.release-x86_64/cpp.lib.algo.crc32.o -msse4.2 -D AOS_SSE42 -O3 -fno-strict-aliasing -Werror -D_MT -D _GNU_SOURCE -Wno-cast-function-type
g++ -x c++ -Wno-invalid-offsetof -ffunction-sections -fdata-sections -Wsequence-point -Wno-multichar -Wall -Wshadow -pipe -Wextra -Winit-self -Wmissing-include-dirs -Wmissing-declarations -Winvalid-pch -iquote . -Wno-vla -fpch-preprocess -Wno-class-memaccess -Wpedantic -c cpp/lib/algo/errtext.cpp -o build/CYGWIN_NT-g++.release-x86_64/cpp.lib.algo.errtext.o -msse4.2 -D AOS_SSE42 -O3 -fno-strict-aliasing -Werror -D_MT -D _GNU_SOURCE -Wno-cast-function-type
g++ -x c++ -Wno-invalid-offsetof -ffunction-sections -fdata-sections -Wsequence-point -Wno-multichar -Wall -Wshadow -pipe -Wextra -Winit-self -Wmissing-include-dirs -Wmissing-declarations -Winvalid-pch -iquote . -Wno-vla -fpch-preprocess -Wno-class-memaccess -Wpedantic -c cpp/lib/algo/file.cpp -o build/CYGWIN_NT-g++.release-x86_64/cpp.lib.algo.file.o -msse4.2 -D AOS_SSE42 -O3 -fno-strict-aliasing -Werror -D_MT -D _GNU_SOURCE -Wno-cast-function-type
